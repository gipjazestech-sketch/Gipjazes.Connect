{
  "name": "gipjazes-connect",
  "version": "1.0.0",
  "scripts": {
    "install-all": "npm install --prefix backend && npm install --prefix web",
    "build-web": "npm run build --prefix web",
    "prestart": "npm run build-web",
    "start": "cd backend && npm start",
    "backend": "npm start --prefix backend",
    "frontend": "npm run dev --prefix web",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
